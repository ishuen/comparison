    "csv-parse": "~2.0.4",
    "debug": "~2.6.3",
    "dragula": "3.7.2",
    "express": "~4.15.2",
    "grunt-apidoc":"~0.11",
    "lodash":"~4.17.5",
    "morgan": "~1.8.1",
    "pareto-frontier":"~1.1.1",
    "pg":"~7.4.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2"
