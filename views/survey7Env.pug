extends layout

block content
  div(class='container')
    div(class='row')
      div(class='col-sm-8')
      div(class='col-sm-4' style='background-color: #f5f5f5')
        h4 progress
        div(class='progress')
          if env == 'inu'
            - var str = Number(17 + 14 * (trial - 13)) + '%'
            div(class='progress-bar progress-bar-success' role='progressbar' style='width:' + Number(17 + 14 * (trial - 13)) + '%;' aria-valuenow= Number(17 + 14 * (trial - 13)) aria-valuemin='0' aria-valuemax='100') #{str}
    div(class='row')
      div(class='col-sm-12')
        iframe(src='https://docs.google.com/forms/d/e/1FAIpQLScPApgwh6_qtUwsAo1Uk45ujKaebLeKWpJrVII-ypwpVrDRqA/viewform?embedded=true&entry.1905076335=' + userId + '&entry.2128922355=' + trial width='100%' height='600' frameborder='0' marginheight='0' marginwidth='0') Loading...